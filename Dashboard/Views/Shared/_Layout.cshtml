    <script type="text/javascript">
          window.paceOptions = {
            document: true,
            eventLag: true,
            restartOnPushState: true,
            restartOnRequestAfter: true,
            ajax: {
              trackMethods: [ 'POST','GET']
            }
          };
    </script>

    <!-- build:js({.tmp,app}) scripts/app.min.js -->
    @Scripts.Render("~/Content/template/vendor/jquery/dist/jquery.js")
    @Scripts.Render("~/Content/template/vendor/tether/dist/js/tether.js")
    @Scripts.Render("~/Content/template/vendor/bootstrap/dist/js/bootstrap.js")
    @Scripts.Render("~/Content/template/vendor/fastclick/lib/fastclick.js")
    @Scripts.Render("~/Content/template/scripts/constants.js")
    @Scripts.Render("~/Content/template/scripts/main.js")
    <!-- endbuild -->
    <!-- page scripts -->
    @Scripts.Render("~/Content/template/vendor/flot/jquery.flot.js")
    @Scripts.Render("~/Content/template/vendor/flot/jquery.flot.resize.js")
    @Scripts.Render("~/Content/template/vendor/flot/jquery.flot.stack.js")
