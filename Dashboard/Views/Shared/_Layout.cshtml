                        </a>
                        <div class="dropdown-divider"></div>
                        <a class="dropdown-item" href="javascript:;">Help</a>
                        <a class="dropdown-item" href="Login/Logout">Logout</a>
                    </div>
                </div>
                <!-- main navigation -->
                <nav>
                    <p class="nav-title">NAVIGATION</p>
                    <ul class="nav">
                        <!-- dashboard -->
                        <li>
                            <a href="@Url.Action("Index", "Home")">
                                <i class="material-icons text-primary">home</i>
                                <span>Startseite</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Lieferant")">
                                <i class="material-icons text-primary">home</i>
                                <span>Lieferanten</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Warengruppe")">
                                <i class="material-icons text-primary">home</i>
                                <span>Warengruppen</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Artikel")">
                                <i class="material-icons text-primary">home</i>
                                <span>Artikel</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "Beleg")">
                                <i class="material-icons text-primary">home</i>
                                <span>Beleg</span>
                            </a>
                        </li>
                        <li>
                            <a href="@Url.Action("Index", "ArtikelBeleg")">
