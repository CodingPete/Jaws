@model IEnumerable<Schichtplaner.Jaws_Service.Schicht>

@{
    ViewBag.Title = "Schichtplaner";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Endzeit_ist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Endzeit_soll)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Pause)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PersonalId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Startzeit_ist)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Startzeit_soll)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Endzeit_ist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Endzeit_soll)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Pause)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PersonalId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Startzeit_ist)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Startzeit_soll)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
